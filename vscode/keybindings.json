// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+t",
        "command": "raspberry-pi-pico.compileProject"
    },
    {
        "key": "cmd+u",
        "command": "raspberry-pi-pico.runProject"
    },
    {
        "key": "cmd+u",
        "command": "-cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+t",
        "command": "-workbench.action.showAllSymbols"
    }
]